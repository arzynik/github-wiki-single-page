			<% end %>

		<link href="https://cdnjs.cloudflare.com/ajax/libs/highlight.js/9.0.0/styles/<%= highlightjs %>.min.css" rel="stylesheet">
		<style>
			body {
				position: relative;
			}
			.links {
				margin-top: 100px;
			}

			@media (min-width: 1024px) {
				.content {
					padding-left: 250px;
				}
				.links {
					position: fixed;
					margin-left: -50px;
					width: 250px;
					height: 100%;
					overflow-y: auto;
					box-sizing: border-box;
				}
				nav {
					width: 250px;
				}
				nav > ol {
					position: absolute;
					width: 250px;

				}
			}
			@media (max-width: 1023px) {
				nav {
					margin-bottom: 4em;
				}
			}
			ol {
				padding-left: 1.8em;
			}
			.link {
				margin-bottom: 8em;
			}
			h1 {
				margin-bottom: .7em;
				margin-top: 0;
			}
			ol ol {
				list-style-type: lower-roman;
				opacity: .9
			}
			.footer {
				margin-top: 100px;
				margin-bottom: 100px;
				text-align: center;
			}
			.project {
				float: right;
			}
			.hljs {
				background: none;
			}
			.content {
				width: 100%;
			}
			.nav li.active > a {
				font-weight: bold;
			}
			.wrapper {
				max-width: 1024px;
				margin: 0 auto;
			}
			.navbar {
				width: 100%;
				background: rgba(255,255,255,.9);
				padding-top: 1em;
			}
			.navbar hr {
				margin-bottom: 0;
			}
		</style>
	</head>
	<body data-spy="scroll" data-target=".links">
		<div class="wrapper">

			<nav class="navbar navbar-fixed-top">
				<div class="container">
					<a href="<%= project %>" class="project">GitHub Project</a>
					<h1><%= name %></h1>
					<hr>
				</div>
			</nav>

			<div class="links"><nav class="nav nav-pills nav-stacked"><%= nav %></nav></div>
			<div class="container content" id="Home">
				<div class="wiki">
					<%= content %>
				</div>
				<div class="footer">
					Created using <a href="https://github.com/arzynik/github-wiki-single-page">Github Single Page Wiki</a>
				</div>
			</div>
		</div>

		<script src="https://ajax.googleapis.com/ajax/libs/jquery/2.1.3/jquery.min.js"></script>
		<script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/js/bootstrap.min.js"></script>
		<script src="https://cdnjs.cloudflare.com/ajax/libs/highlight.js/9.0.0/highlight.min.js"></script>
		<script>
			hljs.initHighlightingOnLoad();
			$(document).ready(function() {
				$("nav li a[href^='#']").on('click', function(e) {
					e.preventDefault();
					var hash = this.hash;
					$('html, body').animate({
						scrollTop: $(hash).offset().top
					}, 300, function(){
						window.location.hash = hash;
					});
				});
			});
		</script>
	</body>
</html>
